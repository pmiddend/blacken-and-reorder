#!/usr/bin/env sh

curl --silent -XPOST "http://localhost:31338$(pwd)" --data-binary @- > /tmp/reordered

if ! curl --fail --silent -XPOST http://localhost:31337 --data-binary @- </tmp/reordered > /tmp/blackened; then
   cat /tmp/reordered
fi

if [ -s /tmp/blackened ]; then
    cat /tmp/blackened
else
    cat /tmp/reordered
fi
